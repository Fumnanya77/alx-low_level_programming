ifneq (m.h)
CC = gcc
SRC = main.c school.c
OBJ = $(SRC:.c=.o)
NAME = school
RM = rm -f
DEL = rm -f *.o
CFLAGS = -Wall -Werror -Wextra -pedantic

all : $(OBJ)
	$(CC) -Wall -Werror -Wextra -pedantic $(OBJ) -o $(NAME)

.PHONY: clean oclean fclean re

clean:
	$(RM) *~ $(NAME)

oclean:
	$(DEL)

fclean:
	$(RM) *~ $(NAME)
	$(DEL)

re: all
	$(DEL)
endif
