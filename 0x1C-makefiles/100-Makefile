ifneq (m.h)
CC = gcc
SRC = main.c school.c
OBJ = $(SRC:.c=.o)
NAME = school
DEL = rm -f
CFLAGS = -Wall -Werror -Wextra -pedantic

all : $(OBJ)
	$(CC) -Wall -Werror -Wextra -pedantic $(OBJ) -o $(NAME)

.PHONY: clean oclean fclean re

clean:
	$(DEL) *~ $(NAME)

oclean:
	$(DEl) *.o

fclean:
	$(DEL) *~ $(NAME)
	$(DEL) *.o

re: all
	$(DEL) *.o
endif
